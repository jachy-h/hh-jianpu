{
  "name": "as-nmn",
  "version": "0.1.0",
  "private": true,
  "description": "Add Seven - A library for rendering numbered musical notation (jianpu) in web applications",
  "scripts": {
    "dev": "pnpm --filter @as-nmn/web dev",
    "build": "pnpm --filter @as-nmn/core build && pnpm --filter @as-nmn/web build",
    "build:core": "pnpm --filter @as-nmn/core build",
    "build:web": "pnpm --filter @as-nmn/web build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "keywords": [
    "jianpu",
    "numbered-musical-notation",
    "music",
    "sheet-music"
  ],
  "license": "MIT",
